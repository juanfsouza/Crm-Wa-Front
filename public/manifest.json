{
    "manifest_version": 3,
    "name": "WhatsApp Kanban",
    "version": "1.0",
    "description": "Kanban com WhatsApp Web integrado",
    "background": {
      "service_worker": "src/service-worker.js"
    },
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "host_permissions": [
      "http://localhost:3000/*",
      "https://web.whatsapp.com/*"
    ]
  }
  